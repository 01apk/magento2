<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/etc/global/variations.xsd">
  <testCase name="SuggestSearchingResultEntityTest">
    <variation name="SuggestSearchingResultEntityTestVariation1" firstConstraint="assertSuggestSearchingResult" method="test">
      <data name="catalogSearch/data/query_text/value">catalogProductSimple::name</data>
      <data name="catalogSearch/data/num_results">-</data>
      <constraint name="assertSuggestSearchingResult"/>
    </variation>
    <variation name="SuggestSearchingResultEntityTestVariation2" firstConstraint="assertSuggestSearchingResult" method="test">
      <data name="catalogSearch/data/query_text/value">catalogProductSimple::sku</data>
      <data name="catalogSearch/data/num_results">1</data>
      <constraint name="assertSuggestSearchingResult"/>
    </variation>
  </testCase>
</config>

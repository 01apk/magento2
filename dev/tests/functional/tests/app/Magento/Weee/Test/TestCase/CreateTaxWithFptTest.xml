<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../etc/global/variations.xsd">
    <testCase name="CreateTaxWithFptTest">
        <variation name="CreateTaxWithFptTestVariation1">
            <data name="description">Check not taxed FPT display set to Excluding, Description and Including FPT on product with custom option catalog price Excluding Tax</data>
            <data name="configData">shipping_tax_class_taxable_goods,tax_with_fpt_cat_excl_disc_on_excl</data>
            <data name="productData">with_custom_option_and_fpt</data>
            <data name="prices/category_price">70.00</data>
            <data name="prices/fpt_category">10.00</data>
            <data name="prices/fpt_total_category">80.00</data>
            <data name="prices/product_page_price">100.00</data>
            <data name="prices/product_page_fpt">10.00</data>
            <data name="prices/product_page_fpt_total">110.00</data>
            <data name="prices/cart_item_price">100.00</data>
            <data name="prices/cart_item_fpt">10.00</data>
            <data name="prices/cart_item_fpt_total">110.00</data>
            <data name="prices/cart_item_subtotal">100.00</data>
            <data name="prices/cart_item_subtotal_fpt">10.00</data>
            <data name="prices/cart_item_subtotal_fpt_total">110.00</data>
            <data name="prices/grand_total">118.25</data>
            <data name="prices/total_fpt">10.00</data>
            <data name="constraint">assertFptApplied</data>
        </variation>

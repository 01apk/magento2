<repository>
    <storage class="Magento\Review\Test\Repository\Rating">
        <dataset name="default" >
            <field name="rating_code" xsi:type="string">Rating %isolation%</field>
            <field name="stores" xsi:type="array">
                <item name="key_0" xsi:type="string">Main Website/Main Website Store/Default Store View</item>
            </field>
            <field name="is_active" xsi:type="string">Yes</field>
        </dataset>
        <dataset name="visibleOnDefaultWebsite" >
            <field name="rating_code" xsi:type="string">productRating_%isolation%</field>
            <field name="stores" xsi:type="array">
                <item name="key_0" xsi:type="string">Main Website/Main Website Store/Default Store View</item>
            </field>
            <field name="is_active" xsi:type="string">Yes</field>
        </dataset>
    </storage>
</repository>

<repository>
    <storage class="Magento\Newsletter\Test\Repository\Template">
        <dataset name="default" >
            <field name="code" xsi:type="string">Newsletter Template %isolation%</field>
            <field name="subject" xsi:type="string">Newsletter Subject %isolation%</field>

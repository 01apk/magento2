<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/etc/global/variations.xsd">
  <testCase name="CreateTaxRateEntityTest">
    <variation name="CreateTaxRateEntityTestVariation1" firstConstraint="assertTaxRateSuccessSaveMessage" method="test">
      <data name="taxRate/data/code">TaxIdentifier%isolation%</data>
      <data name="taxRate/data/zip_is_range">No</data>
      <data name="taxRate/data/zip_from">-</data>
      <data name="taxRate/data/zip_to">-</data>
      <data name="taxRate/data/tax_postcode">*</data>
      <data name="taxRate/data/tax_country_id">Australia</data>
      <data name="taxRate/data/tax_region_id">*</data>
      <data name="taxRate/data/rate">20</data>
      <constraint name="assertTaxRateSuccessSaveMessage" next="assertTaxRateInGrid"/>
      <constraint name="assertTaxRateInGrid" next="assertTaxRateForm" prev="assertTaxRateSuccessSaveMessage"/>
      <constraint name="assertTaxRateForm" next="assertTaxRateInTaxRule" prev="assertTaxRateInGrid"/>
      <constraint name="assertTaxRateInTaxRule" prev="assertTaxRateForm"/>
    </variation>
    <variation name="CreateTaxRateEntityTestVariation2" firstConstraint="assertTaxRateSuccessSaveMessage" method="test">
      <data name="taxRate/data/code">TaxIdentifier%isolation%</data>
      <data name="taxRate/data/zip_is_range">Yes</data>
      <data name="taxRate/data/zip_from">90001</data>
      <data name="taxRate/data/zip_to">96162</data>
      <data name="taxRate/data/tax_postcode">-</data>
      <data name="taxRate/data/tax_country_id">United States</data>
      <data name="taxRate/data/tax_region_id">California</data>
      <data name="taxRate/data/rate">15.5</data>
      <constraint name="assertTaxRateSuccessSaveMessage" next="assertTaxRateInGrid"/>
      <constraint name="assertTaxRateInGrid" next="assertTaxRateForm" prev="assertTaxRateSuccessSaveMessage"/>
      <constraint name="assertTaxRateForm" next="assertTaxRateInTaxRule" prev="assertTaxRateInGrid"/>
      <constraint name="assertTaxRateInTaxRule" prev="assertTaxRateForm"/>
    </variation>
    <variation name="CreateTaxRateEntityTestVariation3" firstConstraint="assertTaxRateSuccessSaveMessage" method="test">
      <data name="taxRate/data/code">TaxIdentifier%isolation%</data>
      <data name="taxRate/data/zip_is_range">No</data>
      <data name="taxRate/data/zip_from">-</data>
      <data name="taxRate/data/zip_to">-</data>
      <data name="taxRate/data/tax_postcode">180</data>
      <data name="taxRate/data/tax_country_id">Canada</data>
      <data name="taxRate/data/tax_region_id">*</data>
      <data name="taxRate/data/rate">25</data>
      <constraint name="assertTaxRateSuccessSaveMessage" next="assertTaxRateInGrid"/>
      <constraint name="assertTaxRateInGrid" next="assertTaxRateForm" prev="assertTaxRateSuccessSaveMessage"/>
      <constraint name="assertTaxRateForm" next="assertTaxRateInTaxRule" prev="assertTaxRateInGrid"/>
      <constraint name="assertTaxRateInTaxRule" prev="assertTaxRateForm"/>
    </variation>
    <variation name="CreateTaxRateEntityTestVariation4" firstConstraint="assertTaxRateIsInCorrectRange" method="test">
      <data name="taxRate/data/code">TaxIdentifier%isolation%</data>
      <data name="taxRate/data/zip_is_range">Yes</data>
      <data name="taxRate/data/zip_to">7800935</data>
      <data name="taxRate/data/tax_postcode">-</data>
      <data name="taxRate/data/tax_country_id">United Kingdom</data>
      <data name="taxRate/data/tax_region_id">*</data>
      <constraint name="assertTaxRateIsInCorrectRange"/>
    </variation>
    <variation name="CreateTaxRateEntityTestVariation5" firstConstraint="assertTaxRateIsInCorrectRange" method="test">
      <data name="taxRate/data/code">TaxIdentifier%isolation%</data>
      <data name="taxRate/data/zip_is_range">No</data>
      <data name="taxRate/data/zip_from">-</data>
      <data name="taxRate/data/zip_to">-</data>
      <data name="taxRate/data/tax_postcode">*</data>
      <data name="taxRate/data/tax_country_id">France</data>
      <data name="taxRate/data/tax_region_id">Val-d'Oise</data>
      <data name="taxRate/data/rate">999</data>
      <constraint name="assertTaxRateIsInCorrectRange"/>
    </variation>
  </testCase>
</config>

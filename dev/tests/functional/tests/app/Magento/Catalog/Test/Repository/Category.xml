<repository xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/Mtf/Repository/etc/repository.xsd">
    <storage class="Magento\Catalog\Test\Repository\Category">
        <dataset name="default" >
            <field name="config" xsi:type="string">
            </field>
            <field name="data" xsi:type="string">
            </field>
        </dataset>
        <dataset name="anchor_category" >
            <field name="config" xsi:type="string">
            </field>
            <field name="data" xsi:type="array">
                <item name="fields" xsi:type="array">
                    <item name="is_anchor" xsi:type="array">
                        <item name="value" xsi:type="string">Yes</item>
                        <item name="input_value" xsi:type="string">1</item>
                        <item name="group" xsi:type="string">display_setting</item>
                        <item name="input" xsi:type="string">select</item>
                    </item>
                </item>
            </field>
        </dataset>
    </storage>
</repository>
